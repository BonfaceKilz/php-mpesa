{
    "name": "hackdelta/mpesa",
    "description": "Php library to wrap around mpesa APIs",
    "type": "library",
    "keywords": [
        "mpesa",
        "php",
        "hackdelta"
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ghostscypher",
            "email": "bngetich69@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hackdelta\\Mpesa\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
